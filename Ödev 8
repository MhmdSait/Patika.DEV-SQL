3.1. UPDATE employee
  SET name = 'Atik',
    birthday = '2000-02-20',
    email = 'atik@get.org'
  WHERE id = 5
  RETURNING *;
  
3.2. UPDATE employee
  SET name = 'Kan',
    email = 'kangroo'
  WHERE name ILIKE '%A'
  RETURNING *;

3.3. UPDATE employee
  SET email = 'GEZGİN@kan'
  WHERE name = 'Kan'
  RETURNING *;
  
3.4. UPDATE employee
  SET birthday = '2222-02-22',
    name = 'ikiningücü'
  WHERE birthday > '2005-01-01'
  RETURNING *;

3.5. UPDATE employee
  SET id = '53',
    name = 'akıl',
    email = 'sanat@ittifak'
  WHERE birthday = '2222-02-22' OR email = 'amcgiffie0@npr.org'
  RETURNING *;
  
 -------------------------------------------------------- 
  
4.1. DELETE FROM employee
  WHERE id = 10
  RETURNING *;
  
4.2. DELETE FROM employee
  WHERE id < 15
  RETURNING *;
  
4.3. DELETE FROM employee
  WHERE birthday < '1980-01-01'
  RETURNING *;  
  
4.4. DELETE FROM employee
  WHERE email LIKE '%n'
  RETURNING *;  
  
4.5. DELETE FROM employee
WHERE name = 'kan' OR birthday > '1995-01-01'
RETURNING *;  
